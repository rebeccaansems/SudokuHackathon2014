   public static int getBoxNumber(int[][][] m,int row, int column) {
      int size = (int) Math.pow(m.length, 0.5); //Find length of box.
      return ((row/size)*size+column/size);
   }
   
   public static int[] reverseSort(int[] l) {
      Arrays.sort(l);
      int[] a = new int[l.length];
      int i = 0;
      while (i<l.length){
         a[a.length-1-i] = l[i];
         i += 1;
      }
      return a;
   }
   
   
