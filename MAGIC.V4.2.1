            if (m[i][j][1] != 0) {
               //System.out.println(i+" "+j); 
               box = numbersInBox(m)[getBoxNumber(m,i,j)];
               //for (int randomVariable=0;randomVariable<box.length;randomVariable++)
                  //System.out.print(box[randomVariable]);
