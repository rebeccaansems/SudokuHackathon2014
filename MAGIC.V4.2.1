   public static int[][] numbersInColumn(int m[][][]) {
      int size = m.length;
      int[][] nums = new int[size][size];
      int i = 0;
      while (i < size) {
         int j = 0;
         while (j < size) {
            if (m[i][j][1] == 0)
               nums[j][(m[i][j][0])-1] = m[i][j][0];
            j += 1;
         }
         i += 1;
      }
      return nums;       
   }
   
